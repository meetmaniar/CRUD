{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "Id": "Product Schema",
  "Product": {
    "id": {
      "default": 1,
      "description": "Product ID",
      "id": "/properties/id",
      "title": "The id schema",
      "type": "integer"
    },
    "name": {
      "default": "A green door",
      "description": "Product Name",
      "id": "/properties/name",
      "title": "The name schema",
      "type": "string"
    },
    "price": {
      "default": 12.5,
      "description": "Product Price",
      "id": "/properties/price",
      "title": "The price schema",
      "type": "number"
    }
